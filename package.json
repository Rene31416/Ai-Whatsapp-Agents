{
  "name": "ai-agents",
  "version": "0.1.0",
  "bin": {
    "ai-agents": "bin/ai-agents.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && esbuild src/app/lambda-handlers/*.ts --bundle --platform=node --target=node20 --outdir=dist --external:aws-sdk",
    "watch": "esbuild src/app/lambda-handlers/*.ts --bundle --platform=node --target=node20 --outdir=dist --watch --external:aws-sdk",
    "start": "ts-node src/app/main.ts",
    "dash": "ts-node src/devtools/dash-log-call/index.ts",
    "dash:demo": "ts-node src/devtools/dash-log-call/index.ts --tenant 762197213646084 --user 50373145544",
    "test": "jest",
    "cdk": "cdk deploy",
    "local": "npx ts-node src/poc/poc-fats.ts"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.1005.0",
    "esbuild": "^0.24.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.28.1",
    "@aws-sdk/client-dynamodb": "^3.908.0",
    "@aws-sdk/client-scheduler": "^3.911.0",
    "@aws-sdk/client-secrets-manager": "^3.906.0",
    "@aws-sdk/client-sqs": "^3.908.0",
    "@aws-sdk/util-dynamodb": "^3.913.0",
    "@google/generative-ai": "^0.24.1",
    "@langchain/core": "^0.3.0",
    "@langchain/google-genai": "^0.2.18",
    "@langchain/langgraph": "^0.4.9",
    "aws-cdk-lib": "2.181.1",
    "axios": "^1.12.2",
    "constructs": "^10.0.0",
    "crypto": "^1.0.1",
    "inversify": "^7.7.1",
    "langchain": "^0.3.0",
    "readline": "^1.3.0",
    "ts-lambda-api": "^2.5.2",
    "zod": "^4.1.12"
  }
}
